package com.zhilianbao.erp.auth.impl.systemparameter;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.zhilianbao.erp.auth.mapper.systemparameter.BaseAreaMapper;
import com.zhilianbao.erp.auth.service.systemparameter.IBaseAreaService;
import com.zhilianbao.erp.auth.vo.systemparameter.BaseAreaVo;
import com.zhilianbao.erp.common.vo.ResponseResult;
/**
 * 
 * @author Tobin
 *
 */
@Service
public class BaseAreaServiceImpl implements IBaseAreaService{

    @Autowired
    private BaseAreaMapper baseAreaMapper;
	@Override
	public ResponseResult<BaseAreaVo> queryArea(BaseAreaVo vo) {
		// TODO Auto-generated method stub
		List<BaseAreaVo> baseAreaVos=baseAreaMapper.queryArea(vo.getId());
		
		ResponseResult<BaseAreaVo> respResult=new ResponseResult<BaseAreaVo>();
		
		respResult.setRows(baseAreaVos);
		return respResult;
	}
	@Override
	public List<BaseAreaVo> mulArea(List<Long> list) {
		// TODO Auto-generated method stub
		return baseAreaMapper.mulArea(list);
	}

}
