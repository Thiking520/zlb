package com.zhilianbao.erp.auth.vo.goods;

import org.apache.commons.lang.builder.ToStringBuilder;

import com.zhilianbao.erp.common.util.LogBeanNoNullStyle;
import com.zhilianbao.erp.common.vo.BaseVo;

public class GoodsMaterialListVo extends BaseVo {

	private static final long serialVersionUID = -6654080176640980805L;

	private Long materialId;
    private Long goodsId;
    private Integer type;
    private Integer sortIndex;


    // -----------------品相册或商品详情页签用到的字段-----------------
    private String fileUrl;//文件URL


    
    
    
	/** 
	 * 获取
	 * @return type 
	 */
	public Integer getType() {
		return type;
	}




	/** 
	 * 设置
	 * @param type 
	 */
	public void setType(Integer type) {
		this.type = type;
	}




	/** 
	 * 获取
	 * @return sortIndex 
	 */
	public Integer getSortIndex() {
		return sortIndex;
	}




	/** 
	 * 设置
	 * @param sortIndex 
	 */
	public void setSortIndex(Integer sortIndex) {
		this.sortIndex = sortIndex;
	}




	/** 
	 * 获取
	 * @return fileUrl 
	 */
	public String getFileUrl() {
		return fileUrl;
	}




	/** 
	 * 设置
	 * @param fileUrl 
	 */
	public void setFileUrl(String fileUrl) {
		this.fileUrl = fileUrl;
	}




	/** 
	 * 获取
	 * @return materialId 
	 */
	public Long getMaterialId() {
		return materialId;
	}




	/** 
	 * 设置
	 * @param materialId 
	 */
	public void setMaterialId(Long materialId) {
		this.materialId = materialId;
	}




	/** 
	 * 获取
	 * @return goodsId 
	 */
	public Long getGoodsId() {
		return goodsId;
	}




	/** 
	 * 设置
	 * @param goodsId 
	 */
	public void setGoodsId(Long goodsId) {
		this.goodsId = goodsId;
	}


	@Override
	public String toString() {
		return ToStringBuilder.reflectionToString(this, new LogBeanNoNullStyle());
	}
}