package com.zhilianbao.erp.auth.entity.archives;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

import com.zhilianbao.erp.common.entity.BaseBean;

@Table(name = "t_delivery_scope")
public class DeliveryScopeBean extends BaseBean implements Serializable{
    
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    /**
     * 站点id
     */
    @Column(name = "delivery_record_id")
    private Long deliveryRecordId;

    /**
     * 运营商id
     */
    @Column(name = "operator_id")
    private Long operatorId;
    /**
     * 省份
     */
    private Long province;

    /**
     * 城市
     */
    private Long city;

    /**
     * 类型   1启用  2 排除
     */
    @Column(name = "is_delivery")
    private Integer isDelivery;
    /**
     * 区、县
     */
    private Long area;

    /**
     * 街道
     */
    private Long street;

    /**
     * 级别
            10：市
            20：区
            30：街道
            40：村
            50：路
            60：小区
            70：写字楼
     */
    private String level;

    /**
     * 名称
     */
    private String name;

    
    private String deliveryRecordType;

    /**
     * @return id
     */
    public Long getId() {
        return id;
    }

    /**
     * @param id
     */
    public void setId(Long id) {
        this.id = id;
    }

    /**
     * 获取站点id
     *
     * @return delivery_record_id - 站点id
     */
    public Long getDeliveryRecordId() {
        return deliveryRecordId;
    }

    /**
     * 设置站点id
     *
     * @param deliveryRecordId 站点id
     */
    public void setDeliveryRecordId(Long deliveryRecordId) {
        this.deliveryRecordId = deliveryRecordId;
    }

    /**
     * 获取省份
     *
     * @return province - 省份
     */
    public Long getProvince() {
        return province;
    }

    /**
     * 设置省份
     *
     * @param province 省份
     */
    public void setProvince(Long province) {
        this.province = province;
    }

    /**
     * 获取城市
     *
     * @return city - 城市
     */
    public Long getCity() {
        return city;
    }

    /**
     * 设置城市
     *
     * @param city 城市
     */
    public void setCity(Long city) {
        this.city = city;
    }

    /**
     * 获取区、县
     *
     * @return area - 区、县
     */
    public Long getArea() {
        return area;
    }

    /**
     * 设置区、县
     *
     * @param area 区、县
     */
    public void setArea(Long area) {
        this.area = area;
    }

    /**
     * 获取街道
     *
     * @return street - 街道
     */
    public Long getStreet() {
        return street;
    }

    /**
     * 设置街道
     *
     * @param street 街道
     */
    public void setStreet(Long street) {
        this.street = street;
    }

    /**
     * 获取级别
            10：市
            20：区
            30：街道
            40：村
            50：路
            60：小区
            70：写字楼
     *
     * @return level - 级别
            10：市
            20：区
            30：街道
            40：村
            50：路
            60：小区
            70：写字楼
     */
    public String getLevel() {
        return level;
    }

    /**
     * 设置级别
            10：市
            20：区
            30：街道
            40：村
            50：路
            60：小区
            70：写字楼
     *
     * @param level 级别
            10：市
            20：区
            30：街道
            40：村
            50：路
            60：小区
            70：写字楼
     */
    public void setLevel(String level) {
        this.level = level;
    }

    /**
     * 获取名称
     *
     * @return name - 名称
     */
    public String getName() {
        return name;
    }

    /**
     * 设置名称
     *
     * @param name 名称
     */
    public void setName(String name) {
        this.name = name;
    }

	public Long getOperatorId() {
		return operatorId;
	}

	public void setOperatorId(Long operatorId) {
		this.operatorId = operatorId;
	}

	public Integer getIsDelivery() {
		return isDelivery;
	}

	public void setIsDelivery(Integer isDelivery) {
		this.isDelivery = isDelivery;
	}

	public String getDeliveryRecordType() {
		return deliveryRecordType;
	}

	public void setDeliveryRecordType(String deliveryRecordType) {
		this.deliveryRecordType = deliveryRecordType;
	}
}