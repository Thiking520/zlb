package com.zhilianbao.erp.auth.impl.systemparameter.facade;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import com.zhilianbao.erp.auth.service.systemparameter.IBaseAreaService;
import com.zhilianbao.erp.auth.service.systemparameter.facade.IBaseAreaRestService;
import com.zhilianbao.erp.auth.vo.systemparameter.BaseAreaVo;
import com.zhilianbao.erp.auth.vo.systemparameter.facade.ParaAreaVo;
import com.zhilianbao.erp.common.vo.ResponseValue;

/**
 * @author Tobin
 * @version 创建时间：2017年4月1日 下午4:30:19 类说明
 */
public class BaseAreaRestServiceImpl implements IBaseAreaRestService {
	@Autowired
	private IBaseAreaService baseAreaService;

	@Override
	public ResponseValue<List<BaseAreaVo>> mulArea(ParaAreaVo paraAreaVo) {
		// TODO Auto-generated method stub
		ResponseValue<List<BaseAreaVo>> responseValue=new ResponseValue<List<BaseAreaVo>>();
		if(paraAreaVo == null){
			return responseValue.failure("多区域id信息，参数为空！");
		}
		List<BaseAreaVo> areaList=baseAreaService.mulArea(paraAreaVo.getList());
		return responseValue.success(areaList);
		
	}

}
