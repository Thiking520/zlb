$(document).ready(function(){
		var topAllMenu=top.allMenu;
		if(topAllMenu){
			for(var i=0; i<topAllMenu.length; i++)  
			  {  
				 if(topAllMenu[i].parentId == 0){
					 $("#topMenu").append('<li onclick="top.next('+topAllMenu[i].id+')"><a href="#"><h2>'+topAllMenu[i].menuName+'</h2></a></li>');
				 }
			     
			  } 
		}
	});