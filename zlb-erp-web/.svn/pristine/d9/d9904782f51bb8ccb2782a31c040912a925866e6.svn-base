	text-align: center;
	line-height: 100px;
}
.L_Page_level{
	width: 100%;
	height: 50px;
	font-size: 16px;
	border: 1px solid #8F8F8F; 
	position: relative;
}
.L_Page_level p{
	position: absolute;
	left: 40px;
	top: 15px;
}

/*发货的流程*/
.L_process{
	width: 95%;
