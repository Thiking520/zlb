package com.zhilianbao.erp.app.vo.wms;

import com.zhilianbao.erp.app.vo.ReqBaseVo;

import java.io.Serializable;

/**
 * Created by jiangfubing on 2017/7/10.
 */
public class ReqSurePick extends ReqBaseVo implements Serializable {

    private static final long serialVersionUID = 6809417725070603569L;

    public String getPickNo() {
        return pickNo;
    }

    public void setPickNo(String pickNo) {
        this.pickNo = pickNo;
    }

    public String getGoodsNo() {
        return goodsNo;
    }

    public void setGoodsNo(String goodsNo) {
        this.goodsNo = goodsNo;
    }

    private String pickNo; //拣货编号
    private String goodsNo; //商品编码

    public String getPickLocation() {
        return pickLocation;
    }

    public void setPickLocation(String pickLocation) {
        this.pickLocation = pickLocation;
    }

    private String pickLocation;//拣货库位
}
