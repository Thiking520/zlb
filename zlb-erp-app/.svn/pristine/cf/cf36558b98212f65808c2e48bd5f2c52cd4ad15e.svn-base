package com.zhilianbao.erp.app.vo.dispatchvehicle;

import com.zhilianbao.erp.app.vo.ReqBaseVo;

/**
* @company zhilianbao
* @author chengjianhui
* @date 2017年3月17日下午2:42:38
* @description:签收入参实体
*/
public class ReqSignSureVo extends ReqBaseVo{
	 /**
     * 签收类型:0:正常签收，1：异常签收
     */
    private int sgnType;
    
	/**
     * 异常签收种类，1：联系不到客户，2：客户拒收
     */
    private int signExceptionType;
    
    /**
     * 异常签收原因
     */
    private String signExceptionDesc;
    
    /**
     * 异常处理方案
     */
    private String signExceptionScheme;
    
    private String driverNo;//运单号
    private String pieNo;	//派车单号
    
    public int getSgnType() {
		return sgnType;
	}

	public void setSgnType(int sgnType) {
		this.sgnType = sgnType;
	}

	public int getSignExceptionType() {
		return signExceptionType;
	}

	public void setSignExceptionType(int signExceptionType) {
		this.signExceptionType = signExceptionType;
	}

	
	public String getSignExceptionDesc() {
		return signExceptionDesc;
	}

	public void setSignExceptionDesc(String signExceptionDesc) {
		this.signExceptionDesc = signExceptionDesc;
	}

	public String getSignExceptionScheme() {
		return signExceptionScheme;
	}

	public void setSignExceptionScheme(String signExceptionScheme) {
		this.signExceptionScheme = signExceptionScheme;
	}

	public String getDriverNo() {
		return driverNo;
	}

	public void setDriverNo(String driverNo) {
		this.driverNo = driverNo;
	}

	public String getPieNo() {
		return pieNo;
	}

	public void setPieNo(String pieNo) {
		this.pieNo = pieNo;
	}
	
}
 