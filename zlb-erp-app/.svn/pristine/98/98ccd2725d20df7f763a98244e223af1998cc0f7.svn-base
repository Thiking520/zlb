public class RspLoadUpVo {
	 /**
     *已经确认装车总数
     */
    private int sureTruckAmount;

	/**
     * 装车总数
     */
    private int truckAmount;
    
    public RspLoadUpVo(){}
    
    public RspLoadUpVo(int sureTruckAmount,int truckAmount){
    	this.sureTruckAmount = sureTruckAmount;
    	this.truckAmount = truckAmount;
    }
    
    public int getSureTruckAmount() {
		return sureTruckAmount;
	}

	public void setSureTruckAmount(int sureTruckAmount) {
		this.sureTruckAmount = sureTruckAmount;
	}

	public int getTruckAmount() {
		return truckAmount;
	}

	public void setTruckAmount(int truckAmount) {
		this.truckAmount = truckAmount;
	}
}
 