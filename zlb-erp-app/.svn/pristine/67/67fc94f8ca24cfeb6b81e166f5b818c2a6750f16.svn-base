package com.zhilianbao.erp.app.vo.dispatchvehicle;

import com.zhilianbao.erp.app.vo.ReqBaseVo;

/**
* @company zhilianbao
* @author chengjianhui
* @date 2017年3月17日下午2:42:38
* @description:装车或移除装车入参实体
*/
public class ReqLoadUpVo extends ReqBaseVo{
	 /**
     * True:确认装车,False:移除确认装车
     */
    private Boolean isTruck;

	/**
     * True:全部确认,False：单个确认
     */
    private Boolean isAll;
    
    private String driverNo;//	子单号
    private String pieNo;//	派车单号
    
  
	public Boolean getIsTruck() {
		return isTruck;
	}

	public void setIsTruck(Boolean isTruck) {
		this.isTruck = isTruck;
	}

	public Boolean getIsAll() {
		return isAll;
	}

	public void setIsAll(Boolean isAll) {
		this.isAll = isAll;
	}

	public String getDriverNo() {
		return driverNo;
	}

	public void setDriverNo(String driverNo) {
		this.driverNo = driverNo;
	}

	public String getPieNo() {
		return pieNo;
	}

	public void setPieNo(String pieNo) {
		this.pieNo = pieNo;
	}

}
 